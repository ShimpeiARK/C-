#include <iostream>
#include <algorithm>
#include <cmath>
#include <limits>
#include <vector>
#include <cstdio>
#include <bits/stdc++.h>
#include <set>
#include <map>
#include <stdio.h>
#include <stack>
#include <queue>
#include <deque>
#include <numeric>
#include <bits/stdc++.h>
#include <utility>
#include <iomanip>

#define ALL(obj) (obj).begin(), (obj).end()
#define FOR(i,a,b) for(int i = (a); i < (b); i++)
#define RFOR(i,a,b) for(int i = (a); (b) <= i; i--)
#define REP(i,n) for(int i = 0; i < (n); i++)
#define RREP(i,n) for(int i = n; n <= i; i--)
#define ABS(a) ((a < 0) ? ((-1)*(a)) : (a))
#define elif else if
#define MOD 1000000007
#define INF (1<<29)

using namespace std;

#define ld long double
#define ll long long

map <int ,int> mpa,mpb;
typedef pair<ll, ll> P;
priority_queue<P, vector<P>, greater<P>> pque;

int main(){
  ios::sync_with_stdio(false);
  cin.tie(NULL);
  ll N,K;
  cin >> N >> K;
  vector<ll> A(N);

  for(int i=0;i<N;i++){
    cin >> A[i];
  }
  ll sum=0;
  for(int i=62;i>=0;i--){
    ll x=0;
    for(int j=0;j<N;j++){
      x+=(A[j]>> i &1);
    }
    if(N>2*x && (sum|(1LL<<i))<=K){
      sum|=1LL<<i;
    }
  }
  ll ans=0;
  for(int i=0;i<N;i++){
    ans+=sum^A[i];
  }
  cout << ans << endl;
  return 0;
}
// ABC117
=====================================================
ll N,M;
cin >> N >> M;
ll a[2020],b[2020],c[2020];
ll dp[1010];
const ll INFA =1LL<<50;
for(int i=0;i<1010;i++){
    dp[i]=INFA;
}
ll p,q,r;
for(int i=0;i<M;i++){
  cin >> p >> q >> r;
  p--;
  q--;
  r=-r;
  a[i]=p;
  b[i]=q;
  c[i]=r;
}
dp[0]=0;
for(int i=0;i<N-1;i++){
  for(int j=0;j<M;j++){
    if(dp[a[j]]==INFA)continue;
    if(dp[b[j]]>dp[a[j]]+c[j]){
      dp[b[j]]=dp[a[j]]+c[j];
    }
  }
}

ll distance=dp[N-1];

for(int i=0;i<N-1;i++){
  for(int j=0;j<M;j++){
    if(dp[a[j]]==INFA)continue;
    if(dp[b[j]]>dp[a[j]]+c[j]){
      dp[b[j]]=dp[a[j]]+c[j];
    }
  }
}
ll ans=dp[N-1];

if(ans!=distance){
  cout << "inf" << endl;
}else{
  cout << -distance << endl;
}
return 0;
}

// ABC_61_bellman-Ford_dp
=====================================================

vector<pair<int,int>> G[400];
int N,M;
int dst[400];
const int INFA=30000000;

int dijkstra(int s){
  priority_queue<pair<int,int>> Q;
  Q.push({s,0});
  fill(dst,dst+N,INFA);
  dst[s]=0;

  while(!Q.empty()){
    pair<int,int> p=Q.top();
    Q.pop();
    int v=p.first;
    for(auto e:G[v]){
      if(dst[e.first]>dst[v]+e.second){
        dst[e.first]=dst[v]+e.second;
        Q.push({e.first,dst[e.first]});
      }
    }
  }
  int time=0;
  for(int i=0;i<N;i++){
    if(dst[i]==INFA)continue;
    time=max(time,dst[i]);
  }
  return time;
}

int main(){
  ios::sync_with_stdio(false);
  cin.tie(NULL);
  cin >> N >> M;
  int a,b,t;
  for(int i=0;i<M;i++){
    cin >> a >> b >> t;
    a--,b--;
    G[a].push_back({b,t});
    G[b].push_back({a,t});
  }
  int ans=INFA;
  for(int i=0;i<N;i++){
    ans=min(ans,dijkstra(i));
  }
  cout << ans << endl;
  return 0;
}

// ABC_10_dijkstra_vector_priority_queue
=====================================================
const ll INF=10000000000;
int dx[4]={1,-1,0,0};
int dy[4]={0,0,1,-1};

int main(){
  ios::sync_with_stdio(false);
  cin.tie(NULL);
  int H,W,T;
  cin >> H >> W >> T;
  char a[110][110];
  int S,G;
  for(int i=0;i<H;i++){
    for(int j=0;j<W;j++){
      cin >> a[i][j];
      if(a[i][j]=='S'){
        S=i*10+j;
      }else if(a[i][j]=='G'){
        G=i*10+j;
      }
    }
  }
  ll lb=1,ub=T;
  while(lb+1<ub){
    ll mid=(lb+ub)/2;
    ll dp[120][120];
    for(int i=0;i<120;i++){
      for(int j=0;j<120;j++){
        dp[i][j]=INF;
        if(i==j)dp[i][j]=0;
      }
    }
    for(int i=0;i<H;i++){
      for(int j=0;j<W;j++){
        for(int k=0;k<4;k++){
          int n=i+dx[k],m=j+dy[k];
          if(n<0 ||m<0 || n>=H || m>=W)continue;
          ll ins=(a[n][m]=='#'?mid:1);
          dp[i*10+j][n*10+m]=ins;
        }
      }
    }
   for(int k=0;k<120;k++){
     for(int i=0;i<120;i++){
       for(int j=0;j<120;j++){
         dp[i][j]=min(dp[i][j],dp[i][k]+dp[k][j]);
       }
     }
   }
   if(dp[S][G] <=1LL*T) lb=mid;
   else ub=mid;
  }
  cout << lb << endl;
  return 0;
}

// ABC_20_ワーシャル_にぶたん_探索_座標変換
